[general]
prompt-on-quit=true
window-height=68
mouse-display=false
window-width=51
window-valignment=0
max-tab-name-length=100
window-losefocus=false
gtk-prefer-dark-theme=true
window-ontop=false
history-size=1000
open-tab-cwd=true
window-tabbar=true
start-at-login=true
quick-open-command-line='gedit %(file_path)s'
compat-delete='delete-sequence'
scroll-keystroke=true
abbreviate-tab-names=true
display-n=-1
use-default-font=false
use-trayicon=false
window-refocus=false
use-scrollbar=true
window-halignment=0
tab-ontop=false
use-popup=false

[keybindings/local]
new-tab='<Primary>t'
split-tab-vertical='<Shift><Alt>w'
focus-terminal-right='<Shift><Alt>l'
close-tab='<Super>w'
rename-current-tab='<Super>r'
close-terminal='<Shift><Alt>x'
focus-terminal-up='<Shift><Alt>k'
focus-terminal-left='<Shift><Alt>h'
split-tab-horizontal='<Shift><Alt>q'
focus-terminal-down='<Shift><Alt>j'

[keybindings/global]
show-hide='<Shift>space'

[style]
cursor-shape=1

[style/background]
transparency=96

[style/font]
allow-bold=true
style='Monospace 14'
palette='#030306063C3C:#C6C600004949:#ABABF1F15757:#FDFDCDCD5E5E:#52525F5FB8B8:#97976F6F8181:#969686866262:#ECECEFEFFCFC:#6C6C5A5A3030:#D9D94A4A8A8A:#DADAFFFFA8A8:#FEFEE6E6A8A8:#B1B1BDBDF9F9:#FDFDA4A4CCCC:#A4A4BCBC8686:#F6F6FFFFECEC:#ECECEFEFFDFD:#222224243636'
palette-name='Fishtank'
